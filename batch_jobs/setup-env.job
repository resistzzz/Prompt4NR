#!/bin/bash
#SBATCH --partition=gpu
#SBATCH --gpus=1
#SBATCH --job-name=SetupEnv
#SBATCH --ntasks=1
#SBATCH --cpus-per-task=18
#SBATCH --time=00:25:00
#SBATCH --output=setup-env_output_%A.out

module purge
module load 2022
module load Anaconda3/2022.05

# Path of framework
FW=$HOME/Prompt4NR

# Create environment
conda env create -f $FW/recsys_gpu.yml